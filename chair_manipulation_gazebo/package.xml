<?xml version="1.0"?>
<package format="2">
  <name>chair_manipulation_gazebo</name>
  <version>0.0.0</version>
  <description>A package setting up a world for two robots to grasp a chair in gazebo.</description>

  <maintainer email="philipp.badenhoop@hellocodeblog.com">Philipp Badenhoop</maintainer>

  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>ur10_robotiq_2f_140_gazebo</exec_depend>
  <exec_depend>ur10_robotiq_2f_140_moveit_config</exec_depend>
  <exec_depend>gazebo_ros</exec_depend>

  <export>
    <gazebo_ros gazebo_model_path="${prefix}/models"/>
    <gazebo_ros gazebo_media_path="${prefix}/models"/>
  </export>

</package>

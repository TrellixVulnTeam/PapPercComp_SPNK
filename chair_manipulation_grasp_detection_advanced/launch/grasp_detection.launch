<launch>
  <node name="grasp_detection" pkg="chair_manipulation_grasp_detection_advanced" type="grasp_detection_node" output="screen">
    <rosparam file="$(find chair_manipulation_grasp_detection_advanced)/cfg/pipeline.yaml" />
    <rosparam file="$(find chair_manipulation_grasp_detection_advanced)/cfg/gripper/robotiq_2f_140.yaml" />
    <param name="gripper_urdf" command="cat $(find chair_manipulation_grasp_detection_advanced)/cfg/gripper/robotiq_arg2f_140.urdf" />
    <param name="gripper_srdf" command="cat $(find chair_manipulation_grasp_detection_advanced)/cfg/gripper/robotiq_arg2f_140.srdf" />
  </node>
</launch>